<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - ACGT</title>
    <link href='http://fonts.googleapis.com/css?family=Montserrat' rel='stylesheet' type='text/css'>
    @Styles.Render("~/Content/New")
    @Scripts.Render("~/bundles/modernizr")

    <script src="~/Scripts/jquery-1.10.2.js"></script>
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/Directives")
   
    <script src="~/js/bootstrap-datepicker.js"></script>
  
</head>
<body ng-app="acgt">
    <header id="home">
        <div id="home-slider" class="carousel slide carousel-fade" data-ride="carousel">
            <div class="carousel-inner">
                <img src="" />
                <div class="item active" style="background-image: url(/Content/Background.png)">
                    <div class="caption">
                        <h1 class="animated fadeInLeftBig">Welcome to ACGT</h1>
                      
                        <p class="animated fadeInRightBig">Bringing DNA to your footstep</p>
                        <p>We are under construction please bare with us as we make our user experience better</p>
                        <a data-scroll class="btn btn-start animated fadeInUpBig" href="@Url.Action("Order", "Sequencing")"> Order Dna Sequencing</a>
                        <a data-scroll class="btn btn-start animated fadeInUpBig" href="(@Url.Action("Order", "Home")"> Order Oligonucleotides </a>
                    </div>
                </div>
                @RenderBody()
            </div>
            <a id="tohash" href="@Url.Action("Home", "Home")"><i class="fa fa-home">-Enter Website</i></a>
        </div>
    </header>
    <script type="text/javascript" src="js/jquery.js"></script>
    <script type="text/javascript" src="js/bootstrap.min.js"></script>

    <script type="text/javascript" src="js/jquery.inview.min.js"></script>
    <script type="text/javascript" src="js/wow.min.js"></script>
    <script type="text/javascript" src="js/mousescroll.js"></script>
    <script type="text/javascript" src="js/smoothscroll.js"></script>
    <script type="text/javascript" src="js/jquery.countTo.js"></script>
    <script type="text/javascript" src="js/lightbox.min.js"></script>
    <script type="text/javascript" src="js/main.js"></script>

    @*@Html.Partial("~/Views/Shared/navbar/_NavBar.cshtml")


    <!-- /#sidebar-wrapper -->
    <!-- Page Content -->
    <div id="wrap">
        <div class="container-fluid">
            <div style="padding-bottom:100px;" class="row">
                <div class="col-lg-12">
                    @RenderBody()
                </div>
            </div>
        </div>
        <div class="push"></div>
    </div>
    <!-- /#page-content-wrapper -->
    @Html.Partial("~/Views/Shared/footer/_footer.cshtml");*@
</body>

</html>
